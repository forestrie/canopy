{
  "name": "@canopy/api",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc --noEmit && wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:debug": "vitest --inspect --no-file-parallelism",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "cbor-x": "^1.5.9",
    "cose-js": "^0.9.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.13",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.43.0"
  }
}
