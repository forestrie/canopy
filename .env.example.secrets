# Cloudflare R2 API Tokens (REQUIRED - DO NOT COMMIT)
# Admin token for creating/destroying R2 buckets and infrastructure
# Required permissions: Account:Cloudflare R2 Storage:Edit
R2_ADMIN=your_r2_admin_token_here

# Writer token for reading and writing R2 objects (used by app)
# Required permissions: Account:Cloudflare R2 Storage:Edit (scoped to specific buckets)
R2_WRITER=your_r2_writer_token_here

# Reader token for read-only access to R2 objects
# Required permissions: Account:Cloudflare R2 Storage:Read (scoped to specific buckets)
R2_READER=your_r2_reader_token_here

# Queue tokens not used in current design

# API Authentication Keys (REQUIRED for production)
API_KEY_SECRET=your_api_key_secret_here

# External Sequencer Configuration (OPTIONAL)
SEQUENCER_ENDPOINT=https://sequencer.example.com
SEQUENCER_API_KEY=your_sequencer_api_key_here

# MMR Index Service (OPTIONAL - defaults to mock)
MMR_SERVICE_ENDPOINT=https://mmr.example.com
MMR_SERVICE_API_KEY=your_mmr_api_key_here